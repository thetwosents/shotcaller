{
  "name": "node-es6-starter",
  "version": "1.0.0",
  "description": "Node.js ES6 starter with Babel & ESLint",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base"
  },
  "scripts": {
    "lint": "eslint src/**",
    "build": "babel --presets es2015 src -d lib",
    "prepublish": "npm run build",
    "start": "node lib/",
    "dev": "./node_modules/.bin/babel-node src/",
    "test-cov": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "test": "babel-node ./node_modules/.bin/_mocha"
  },
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.8.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/palanik/node-es6-starter.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "N. Palani Kumanan",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.7.0",
    "isparta": "^4.0.0",
    "mocha": "^3.5.0"
  },
  "dependencies": {
    "async": "^2.5.0",
    "body-parser": "^1.17.2",
    "crawler": "^1.0.5",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.4",
    "path": "^0.12.7",
    "puppeteer": "^0.9.0",
    "sanitize-filename": "^1.6.1",
    "sitemap-generator": "^7.1.2",
    "sitemap-stream-parser": "^1.2.2",
    "sitemapper": "^2.1.13"
  }
}
